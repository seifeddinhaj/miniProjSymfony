
{% extends 'adminbase.html.twig' %}

{% block title %}{% endblock %}


{% block sub %}
    All users
{% endblock %}
{% block body %}

<div class="container">
        <div class="table-responsive">
            <table class="table table-hover">
                <thead>
                <tr>
                    <th scope="col">#</th>

                    <th scope="col">Name</th>
                    <th scope="col">Email</th>
                    
                    
                </tr>
                </thead>
                <tbody>
                {% for user in user %}
                <tr>
                    <th scope="row">{{user.id}}</th>
                    <td>{{user.name}}</td>
                    <td>{{user.email}}</td>
                    
                </tr>
                    {% endfor %}

                </tbody>
            </table>
        </div>
    </div>
{% endblock %}